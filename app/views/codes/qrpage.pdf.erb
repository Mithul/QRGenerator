<!doctype html>
<html>
  <head>
    <meta charset='utf-8' />
    <%= wicked_pdf_stylesheet_link_tag "pdf" -%>
    <%= wicked_pdf_javascript_include_tag "number_pages" %>
  </head>
    <body onload='number_pages'>
<% @codes.each do |code| %>
  <% (1..10).each do  %>
    <%= code.qrcode.path %>
    <%= pdf_image_tag code.qrcode.path, class: 'qr' %>
  <% end %>
  <br>
<% end %>
 </body>
</html>
